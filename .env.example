# WordPress API Configuration
VITE_WP_API_URL=https://your-wordpress-site.com/wp-json
VITE_WP_JWT_AUTH_URL=https://your-wordpress-site.com/wp-json/jwt-auth/v1

# Environment
VITE_ENV=development

# API Feature Flags
VITE_ENABLE_COMMENTS=true
VITE_ENABLE_SOCIAL_SHARE=true

# Discord Integration
VITE_DISCORD_WEBHOOK_URL=your_discord_webhook_url_here

# Vext API Configuration
VITE_VEXT_API_KEY=your_vext_api_key_here
VITE_VEXT_ENDPOINT_ID=your_vext_endpoint_id_here

# DeepSeek API Configuration
VITE_DEEPSEEK_API_URL=https://api.deepseek.ai
VITE_DEEPSEEK_API_KEY=your_deepseek_api_key_here